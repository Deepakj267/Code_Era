using System;

namespace ClassLib
{
    public class Mymath
    {
        public static void Main()
        {
            string b;
            byte i;
            Console.WriteLine("Enter number to convert into byte");
            b = Console.ReadLine();

            i = byte.Parse(b);

            Console.WriteLine("following is number after converted into byte:");
            Console.WriteLine(i);
            Console.ReadLine();
        }
    }
}
