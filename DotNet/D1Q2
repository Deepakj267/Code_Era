//Q2. Declare 2  variable of a type byte and print product of it.using System;




using System;
namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {         
            byte x = 10;
            byte y = 20;
            int z=x*y;

            Console.WriteLine(z);
            Console.ReadLine();
        }
    }  
}
